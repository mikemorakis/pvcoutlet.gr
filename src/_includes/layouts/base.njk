{% if showJsonLd %}
  <script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@graph": [
      {
        "@type": "Organization",
        "@id": "{{ site.url }}/#organization",
        "name": "{{ site.name }}",
        "url": "{{ site.url }}/",
        "logo": { "@type": "ImageObject", "url": "{{ site.logo | default(site.url + '/img/pvcoutlet-logo.webp') }}" },
        "sameAs": {{ site.sameAs | dump | safe }}
      },
      {
        "@type": "LocalBusiness",
        "@id": "{{ site.url }}/",
        "name": "{{ site.name }}",
        "image": ["{{ site.url }}/img/pvcoutlet-logo.webp", "{{ site.ogImage }}"],
        "logo": "{{ site.logo | default(site.url + '/img/pvcoutlet-logo.webp') }}",
        "url": "{{ site.url }}/",
        "telephone": "{{ site.phone }}",
        "contactPoint": {
          "@type": "ContactPoint",
          "telephone": "{{ site.phone }}",
          "contactType": "customer service",
          "areaServed": "GR",
          "availableLanguage": "Greek",
          "hoursAvailable": { "@type": "OpeningHoursSpecification", "dayOfWeek": ["Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"], "opens": "09:00", "closes": "17:00" }
        },
        "priceRange": "{{ site.priceRange | default('â‚¬â‚¬') }}",
        "areaServed": { "@type": "Country", "name": "Î•Î»Î»Î¬Î´Î±" },
        "address": {
          "@type": "PostalAddress",
          "streetAddress": "{{ site.address }}",
          "addressLocality": "{{ site.locality }}",
          "postalCode": "{{ site.postalCode }}",
          "addressCountry": "{{ site.country }}"
        },
        "geo": { "@type": "GeoCoordinates", "latitude": {{ site.geo.lat }}, "longitude": {{ site.geo.lng }} },
        "openingHoursSpecification": { "@type": "OpeningHoursSpecification", "dayOfWeek": ["Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"], "opens": "09:00", "closes": "17:00" },
        "sameAs": {{ site.sameAs | dump | safe }},
        "parentOrganization": { "@id": "{{ site.url }}/#organization" }
        {% if site.aggregateRating and site.aggregateRating.ratingValue %},
        "aggregateRating": {
          "@type": "AggregateRating",
          "ratingValue": "{{ site.aggregateRating.ratingValue }}",
          "bestRating": "{{ site.aggregateRating.bestRating | default('5') }}",
          "worstRating": "{{ site.aggregateRating.worstRating | default('1') }}",
          "reviewCount": "{{ site.aggregateRating.reviewCount }}"
        }
        {% endif %}
      }
      {% if faq and faq.length %}
      ,{
        "@type": "FAQPage",
        "mainEntity": [
          {% for item in faq %}
          { "@type": "Question", "name": {{ item.q | dump | safe }}, "acceptedAnswer": { "@type": "Answer", "text": {{ item.a | dump | safe }} }}{% if not loop.last %},{% endif %}
          {% endfor %}
        ]
      }
      {% endif %}
    ]
  }
  </script>
{% endif %}
  <script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "WebPage",
    "name": {{ title | default(site.name) | dump | safe }},
    "description": {{ description | dump | safe }},
    "url": "{{ canonical | default(site.url + page.url) }}",
    "dateModified": "{{ dateModified | default(page.date | dateFormat) | default('2025-02-07') }}",
    "primaryImageOfPage": { "@type": "ImageObject", "url": "{{ site.ogImage }}" },
    "speakable": {
      "@type": "SpeakableSpecification",
      "cssSelector": ["#main", "h1", "h2", "h3", ".intro", ".basii", ".titlos2", ".pvch2"]
    }
  }
  </script>
{% if breadcrumbs and breadcrumbs.length %}
  <script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "BreadcrumbList",
    "itemListElement": [
      {% for b in breadcrumbs %}
      { "@type": "ListItem", "position": {{ loop.index }}, "name": {{ b.name | dump | safe }}, "item": "{{ site.url }}{{ b.url }}" }{% if not loop.last %},{% endif %}
      {% endfor %}
    ]
  }
  </script>
{% endif %}
{% if offerPrice and productName %}
  <script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "Product",
    "name": {{ productName | dump | safe }},
    "description": {{ description | dump | safe }},
    "brand": { "@type": "Brand", "name": "{{ site.name }}" },
    "offers": {
      "@type": "Offer",
      "url": "{{ canonical | default(site.url + page.url) }}",
      "priceCurrency": "EUR",
      "price": "{{ offerPrice }}",
      "priceValidUntil": "{{ offerPriceValidUntil | default('2026-12-31') }}",
      "availability": "https://schema.org/InStock",
      "seller": { "@id": "{{ site.url }}/" }
    }
  }
  </script>
{% endif %}
{% if howToName and howToSteps and howToSteps.length %}
  <script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "HowTo",
    "name": {{ howToName | dump | safe }},
    "description": {{ howToDescription | default(description) | dump | safe }},
    "step": [
    {% for s in howToSteps %}
    { "@type": "HowToStep", "position": {{ loop.index }}, "name": {{ s.name | dump | safe }}, "text": {{ s.text | dump | safe }} }{% if not loop.last %},{% endif %}
    {% endfor %}
    ]
  }
  </script>
{% endif %}
